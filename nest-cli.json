{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/please-ticketing-backend/src",
  "monorepo": true,
  "root": "apps/please-ticketing-backend",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/please-ticketing-backend/tsconfig.app.json"
  },
  "projects": {
    "please-ticketing-backend": {
      "type": "application",
      "root": "apps/please-ticketing-backend",
      "entryFile": "main",
      "sourceRoot": "apps/please-ticketing-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/please-ticketing-backend/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "ticket": {
      "type": "application",
      "root": "apps/ticket",
      "entryFile": "main",
      "sourceRoot": "apps/ticket/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ticket/tsconfig.app.json"
      }
    },
    "event-bus": {
      "type": "application",
      "root": "apps/event-bus",
      "entryFile": "main",
      "sourceRoot": "apps/event-bus/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event-bus/tsconfig.app.json"
      }
    },
    "tickets": {
      "type": "application",
      "root": "apps/tickets",
      "entryFile": "main",
      "sourceRoot": "apps/tickets/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tickets/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}